# Copy to .env and fill in values. Do NOT commit .env.

# Required for real API calls (ADC or env token)
GOOGLE_ADS_DEVELOPER_TOKEN=

# Optional: Quota project used for billing with ADC (helps avoid 403s)
# Typically your active gcloud project ID
GOOGLE_ADS_QUOTA_PROJECT_ID=

# Auth mode (default: adc). Other modes not implemented in TS yet.
# Options: adc | gcloud_cli | oauth | service_account
GOOGLE_ADS_AUTH_TYPE=adc

# Optional: Direct access token for dev/testing (bypasses ADC)
# GOOGLE_ADS_ACCESS_TOKEN=

# Optional: Manager and customer IDs (10 digits, no dashes)
# GOOGLE_ADS_LOGIN_CUSTOMER_ID=
# GOOGLE_ADS_CUSTOMER_ID=

# Optional: Google Ads API version (default v19). Example: v21
# GOOGLE_ADS_API_VERSION=v21
